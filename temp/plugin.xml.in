<?xml version="1.0" encoding="UTF-8"?>
<plugin id="Gnumeric_fnlookup">
	<information>
		<_name>Lookup Functions</_name>
		<_description>Functions for looking up values in ranges</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="plugin"/>
	</loader>
	<services>
		<service type="function_group" id="lookup">
			<_category>Lookup</_category>
			<functions textdomain="gnumeric-VERSION-functions">
				<function name="columnnum"/>
				<function name="rows"/>
			</functions>
		</service>
	</services>
</plugin>
